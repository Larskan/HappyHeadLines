{
  "id": null,
  "uid": "cache-ratio-dashboard",
  "title": "Cache Hit Ratio Dashboard",
  "tags": ["cache", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Comment Cache Hit Ratio",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "comment_cache_hit_ratio",
          "legendFormat": "CommentCache",
          "interval": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Article Cache Hit Ratio",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "article_cache_hit_ratio",
          "legendFormat": "ArticleCache",
          "interval": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 }
    }
  ],
  "templating": { "list": [] },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "hide": true,
        "enable": true
      }
    ]
  }
